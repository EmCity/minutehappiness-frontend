{
  "ConnectionStrings": {
    "DefaultConnection": "[connectionString]"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      { "Name": "LiterateConsole" }
    ]
  },
  "VideoConfig": {
    "UrlConfig": {
      "YouTube": "https://www.youtube.com/embed/{videoId}?autoplay=1&cc_load_policy=0&color=white&controls=0&disablekb=1&enablejsapi=1&end={endSeconds}&fs=0&hl=en&iv_load_policy=3&origin={origin}&modestbranding=1&rel=0&showinfo=0&start={startSeconds}"
    },
    "ParamsConfig": {
      "YouTube": {
        "autoplay": 1,
        "cc_load_policy": 0,
        "color": "white",
        "controls": 0,
        "disablekb": 1,
        "fs": 0,
        "hl": "en",
        "iv_load_policy": 3,
        "modestbranding": 1,
        "rel": 0,
        "showinfo": 0
      }
    }
  }  
}